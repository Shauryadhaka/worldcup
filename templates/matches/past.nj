{% extends 'includes/base.nj' %}

{% block content %}
    {% if pastMatches|length > 0 %}
        {% for match in pastMatches %}
            <div class="col-md-4 col-xs-12">
                <div class="panel panel-default">
                    {% include 'includes/match.nj' %}
                </div>
            </div>
        {% endfor %}
    {% endif %}
{% endblock %}
