{% extends '../includes/base.nj' %}

{% block content %}
    {% if club %}
        {% for player in club.players %}
            <div class="col-md-4 col-xs-12">
                <div class="panel panel-default">
                    {% include 'includes/player.nj' %}
                </div>
            </div>
        {% endfor %}
    {% else %}
        <h4>No info available for this club</h4>
    {% endif %}
{% endblock %}
